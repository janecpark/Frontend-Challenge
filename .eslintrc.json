{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
         "prettier/prettier": ["error",  {"singleQuote": true, "parser": "flow"}],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
